	<modelVersion>4.0.0</modelVersion>
	<artifactId>TrustDashBoard</artifactId>
	<packaging>war</packaging>
	<name>Trust DashBoard UI</name>
	<parent>
		<groupId>com.intel.mtwilson.core.portals</groupId>
		<artifactId>oat-portals</artifactId>
		<version>2.2</version>
	</parent>
	<dependencies>
		<dependency>
			<groupId>com.intel.mtwilson.core.services</groupId>
			<artifactId>trust-util-jar</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
